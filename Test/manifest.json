{
	"name": "Message Timer",
	"version": "1.3",
	"description": "Times how long it takes to send a message to a content script and back.",
	"content_scripts": [
	{
		"matches": ["http://*/*"],
		"js": ["page.js"]
	}
	],
	"browser_action": {
		"default_title": "Time to current page",
		"default_icon": "icon_big.png",
		"default_popup": "popup.html"
	},
	"manifest_version": 2,
	"permissions": [
	"tabs"
	]
}