{
  "manifest_version": 2,
  "name": "MyAd",
  "version": "1.0",
  "description": "MyAd",
  "icons": { "48": "icon_big.png" },
  "content_scripts": [
    {
      "matches": ["http://*.khan.co.kr/*"],
      "js": ["ad_khan.js"],
      "run_at" : "document_end"
    },
	{
      "matches": ["http://*.chosun.com/*"],
      "js": ["ad_chosun.js"],
      "run_at" : "document_end"
    },
	{
      "matches": ["http://*.hani.co.kr/*"],
      "js": ["ad_hani.js"],
      "run_at" : "document_end"
    },
	{
      "matches": ["http://*.mediatoday.co.kr/*"],
      "js": ["ad_mediatoday.js"],
      "run_at" : "document_end"
    },
	{
      "matches": ["http://*.pressian.com/*"],
      "js": ["ad_pressian.js"],
      "run_at" : "document_end"
    }
  ]
}
